#pragma once

// The numeric version format is AAAAABBBBBCCCCCDDDE where:
//
// AAAAA - major version number
// BBBBB - minor version number
// CCCCC - bugfix version number
// DDD   - alpha / beta (DDD + 500) version number
// E     - final (0) / snapshot (1)
//
// When DDDE is not 0, 1 is subtracted from AAAAABBBBBCCCCC. For example:
//
// Version      AAAAABBBBBCCCCCDDDE
//
// 0.1.0        0000000001000000000
// 0.1.2        0000000001000020000
// 1.2.3        0000100002000030000
// 2.2.0-a.1    0000200001999990010
// 3.0.0-b.2    0000299999999995020
// 2.2.0-a.1.z  0000200001999990011
//
#define LIBMCIS17A_VERSION       $libmcis17a.version.project_number$ULL
#define LIBMCIS17A_VERSION_STR   "$libmcis17a.version.project$"
#define LIBMCIS17A_VERSION_ID    "$libmcis17a.version.project_id$"
#define LIBMCIS17A_VERSION_FULL  "$libmcis17a.version$"

#define LIBMCIS17A_VERSION_MAJOR $libmcis17a.version.major$
#define LIBMCIS17A_VERSION_MINOR $libmcis17a.version.minor$
#define LIBMCIS17A_VERSION_PATCH $libmcis17a.version.patch$

#define LIBMCIS17A_PRE_RELEASE   $libmcis17a.version.pre_release$

#define LIBMCIS17A_SNAPSHOT_SN   $libmcis17a.version.snapshot_sn$ULL
#define LIBMCIS17A_SNAPSHOT_ID   "$libmcis17a.version.snapshot_id$"
